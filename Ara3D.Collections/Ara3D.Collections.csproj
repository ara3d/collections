    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>A library of immutable collections.</Description>
    <PackageProjectUrl>https://github.com/ara3d/collections</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ara3d/collections</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Update="README.md">
